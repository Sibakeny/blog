<div class="container">
    <%= form_with url: session_path do |f| %>
    <div class="form-group">
        <%= f.label :email %>
        <%= f.text_field :email, class: 'form-control' %>
    </div>
    <div class="form-group">
        <%= f.label :password %>
        <%= f.password_field :password, class: 'form-control' %>
    </div>
    <div class="form-group">
        <%= f.submit 'ログイン', class: 'btn btn-primary' %>
    </div>
    <% end %>
</div>